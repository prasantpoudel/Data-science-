    "def swap(l):\n",
    "#     for i in range(0,len(l)-1,2):\n",
    "#         l[i],l[i+1]=l[i+1],l[i]\n",
    "#     print(l)\n",
    "    i=0\n",
    "    while i+1<len(l):\n",
    "        l[i],l[i+1]=l[i+1],l[i]\n",
    "        i+=2\n",
    "        \n",
    "l=[1,2,3,4,8]\n",
    "swap(l)\n",
    "print(l)"
